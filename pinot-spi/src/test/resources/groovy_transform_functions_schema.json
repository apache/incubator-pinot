{
  "schemaName": "events",
  "dimensionFieldSpecs": [
    {
      "name": "userId",
      "dataType": "LONG",
      "transformFunction": "Groovy({userID}, userID)"
    },
    {
      "name": "fullName",
      "dataType": "STRING",
      "transformFunction": "Groovy({firstName+' '+lastName}, firstName, lastName)"
    },
    {
      "name": "bids",
      "dataType": "INT",
      "singleValueField": false
    },
    {
      "name": "maxBid",
      "dataType": "INT",
      "transformFunction": "Groovy({bids.max{ it.toBigDecimal() }}, bids)"
    }
  ],
  "metricFieldSpecs": [
    {
      "name": "cost",
      "dataType": "DOUBLE"
    }
  ],
  "timeFieldSpec": {
    "incomingGranularitySpec": {
      "name": "hoursSinceEpoch",
      "dataType": "LONG",
      "timeFormat" : "EPOCH",
      "timeType": "HOURS"
    },
    "transformFunction": "Groovy({timestamp/(1000*60*60)}, timestamp)"
  }
}
