{{#if isSplit}}
  {{#each splitUrns as |urn|}}
    <div>
      <h4>{{get-safe splitLabels urn}}</h4>
      {{timeseries-chart
        series=(get splitSeries urn)
        tooltip=tooltip
        legend=legend
        axis=axis
      }}
    </div>
  {{/each}}
{{else}}
  {{timeseries-chart
    series=series
    tooltip=tooltip
    legend=legend
    axis=axis
  }}
{{/if}}
